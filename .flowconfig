[ignore]

[include]

[libs]
flow-typed

[lints]
all=warn
untyped-import=off
unclear-type=off

[options]
emoji=true
include_warnings=true
munge_underscores=true
experimental.const_params=true

suppress_comment=\\(.\\|\n\\)*\\$FlowFixedInNextDeploy
suppress_comment=\\(.\\|\n\\)*\\$FlowExpectError
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe

[version]
^0.66
